@using Ticket.ViewModels
@model DepartamentosViewModel
@{
    ViewBag.Title = "Departamentos";
}

<div class="row form-group">
    <div class="col-md-12 col-xs-6">
        @Html.ActionLink("Criar", "Criar")
    </div>
</div>
<div class="row form-group">
    <div class="col-md-12 col-xs-6">
        <div class="panel panel-default">
            <div class="panel-heading">
                Pesquisa - @ViewBag.Title
            </div>
            <div class="panel-body">
                <div class="row form-group">
                    <div class="col-lg-12">
                        @Html.Partial("_ParametrosPesquisa", Model)
                    </div> @*-- Fim row _ParametrosPesquisa*@
                </div>
                <div class="row">
                    <div class="col-lg-12">
                        <div id="pesquisa">
                            @Html.Partial("_Pesquisa", Model)
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


